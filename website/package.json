{
  "name": "benchmarks-game",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "=14"
  },
  "scripts": {
    "build": "nuxt generate --force-build",
    "vercel-build": "tar -xvf content.tar.gz && yarn build",
    "dev": "nuxt dev -s -t server -o",
    "start": "nuxt start -H 0.0.0.0",
    "generate": "nuxt generate --force-build",
    "regenerate": "rm -r dist && nuxt generate --force-build",
    "nuxt-help": "nuxt -h",
    "nuxt-generate-help": "nuxt generate -h",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "content:rm": "rm -r content",
    "content:cp": "cp -a ./../bench/build/_results content",
    "archive-content": "tar -czvf content.tar.gz content/**",
    "content": "yarn content:rm && yarn content:cp && yarn archive-content",
    "update-db": "npx browserslist@latest --update-db"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/amp": "^0.5.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "core-js": "^3.21.1",
    "find-versions": "^5.0.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "nuxt-property-decorator": "^2.9.1",
    "scss-google-fonts": "^1.0.0",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue2-touch-events": "^3.2.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@types/lodash": "^4.14.179",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "fibers": "^5.0.1",
    "jest": "^27.5.1",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^10.2.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.3",
    "vue-jest": "^3.0.7"
  }
}
