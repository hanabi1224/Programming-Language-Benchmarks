{
  "name": "benchmarks-game",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "nuxt generate --force-build",
    "vercel-build": "tar -xvf content.tar.gz && pnpm build",
    "vercel-publish": "pnpm archive-content && vc --prod",
    "dev": "nuxt dev -s -t server -o",
    "start": "nuxt start -H 0.0.0.0",
    "generate": "nuxt generate --force-build",
    "regenerate": "rm -r dist && nuxt generate --force-build",
    "nuxt-help": "nuxt -h",
    "nuxt-generate-help": "nuxt generate -h",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "jest",
    "content:rm": "rm -r content",
    "content:cp": "cp -a ./../bench/build/_results content",
    "archive-content": "tar -czvf content.tar.gz content/**",
    "content": "pnpm content:rm && pnpm content:cp && pnpm archive-content",
    "update-db": "npx browserslist@latest --update-db"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/amp": "^0.5.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "core-js": "^3.34.0",
    "find-versions": "^5.1.0",
    "humanizer.node": "^1.0.4",
    "lodash": "^4.17.21",
    "nuxt": "^2.17.2",
    "nuxt-property-decorator": "^2.9.1",
    "scss-google-fonts": "^1.0.0",
    "vue": "^2.7.15",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.7.15",
    "vue2-touch-events": "^3.2.3"
  },
  "devDependencies": {
    "@nuxt/types": "^2.17.2",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.2.2",
    "@nuxtjs/tailwindcss": "5.1.3",
    "@types/lodash": "^4.14.202",
    "@vue/test-utils": "^2.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.19.2",
    "fibers": "^5.0.3",
    "jest": "^28.1.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "sass-loader": "^10.5.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-scss": "^4.7.0",
    "ts-jest": "^28.0.8",
    "vue-jest": "^3.0.7"
  }
}